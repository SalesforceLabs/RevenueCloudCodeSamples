    <description>Creates a quote record for new business on an opportunity.</description>
    <environments>Default</environments>
    <formulas>
        <description>Used to calculate the quote name based on the number of quotes already on the opportunity.</description>
        <name>CalculatedQuoteName</name>
        <dataType>String</dataType>
        <expression>&quot;Quote &quot; + TEXT({!QuoteNumber}) + &quot; for &quot; + {!GetOpportunityRecord.Name}</expression>
    </formulas>
